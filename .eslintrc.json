{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    },
    "env": {
        "es6": true
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ],
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "semi": "warn"
    }
}
